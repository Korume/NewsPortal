  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory>
      <property name="dialect">NHibernate.Dialect.MsSql2012Dialect</property>
      <property name="connection.connection_string">
        Server=newsportal.database.windows.net;Database=NewsPortal;User Id=ISM_NewsPortal_Admin;Password=News_Portal_Password;
        <!--   Удалил локальную бд т.к. оперативней всем работать с онлайн базой -->
      </property>
      <mapping assembly="NewsPortal" />
    </session-factory>
  </hibernate-configuration>

  <appSettings>
    <add key="mailAccount" value="inging234@gmail.com" />
    <add key="mailPassword" value="3m0a0r3i7n6a" />
    <add key="mailHost" value="smtp.gmail.com" />
    <add key="mailPort" value="587" />
    <add key="mailEnableSsl" value="true" />
    <add key="mailDeliveryMethod" value="Network" />
    <add key="mailUseDefaultCredentials" value="false" />

    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
